{
  "name": "coffee-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "sass-dev": "sass --watch --update --style=expanded src/sass:src/sass",
    "sass-prod": "sass --no-source-map --style=compressed src/sass:src/sass",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "serve": "vue-cli-service serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "cors": "^2.8.5",
    "pinia": "^2.0.36",
    "vue": "^3.3.2",
    "vue-router": "^4.2.2",
    "vue-toast-notification": "^3.1.1",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "dotenv": "^16.1.4",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "vite": "^4.3.5"
  }
